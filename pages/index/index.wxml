
        
<block>
    <swiper indicator-dots="{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" style="width:100%; height: {{scrolly}}">
    <block wx:for="{{imgUrls}}" wx:key="id">
        <swiper-item>
        <image src="{{item.url}}" class="slide-image" class="img"/>
        <view class='index-text'>{{item.description}}</view>
        </swiper-item>
    </block>
    </swiper>
</block>


